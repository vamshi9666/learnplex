    "@graphql-codegen/typescript-operations": "1.13.3",
    "@types/cookie": "^0.3.3",
    "@types/draft-js": "^0.10.40",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^13.13.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.34",
