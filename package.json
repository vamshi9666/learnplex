{
  "name": "learning-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "gen": "graphql-codegen --config codegen.yml && yarn lint",
